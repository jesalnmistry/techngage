<!DOCTYPE html>

<html lang='en'>
	<head>
		<title>Techngage</title>
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
		<%= csrf_meta_tags %>
	</head>

	<body>
		<%= render 'shared/navigationbar' %>
		<div id="main-container" class="container" align="center">
			<%= yield %>
		</div>
		
		<div id="static-container" class="container" align="center">
			<h2>static content (text/image) goes here</h2>
		</div>

		<%= render 'shared/footer' %>
	</body>

</html>
